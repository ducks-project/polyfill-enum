{
    "name": "ducks-project/polyfill-enum",
    "description": "Polyfill for PHP enumerations interface.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "Enum",
        "enumerator",
        "POO",
        "UnitEnum",
        "BackedEnum",
        "enum_exists",
        "polyfill",
        "php",
        "compat"
    ],
    "authors": [
        {
            "name": "Adrien Loyant",
            "email": "donald_duck@team-df.org",
            "homepage": "http://adrien.loyant.net",
            "role": "Owner"
        }
    ],
    "homepage": "https://github.com/ducks-project/polyfill-enum/",
    "support": {
        "email": "adrien.loyant@gmail.com"
    },
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.64"
    },
    "repositories": [
        {
            "type": "path",
            "url": "./packages/replace",
            "options": {
                "reference": "none",
                "versions": {
                    "ducks-project/polyfill-enum-subpackage-replace": "1.0.0"
                }
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "Ducks\\Polyfill\\Enum\\": ""
        },
        "classmap": [
            "Resources/stubs"
        ],
        "files": [
            "bootstrap.php"
        ]
    }
}
